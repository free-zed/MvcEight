<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Student Management System", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    @*<li>@Html.ActionLink("Students", "Students", "Home")</li>*@
                    @*<li>@Html.ActionLink("Teachers", "Teachers", "Home")</li>*@
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Students<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("ClassOne", "Index", "ClassOneStudent")</li>
                            <li>@Html.ActionLink("ClassTwo", "Index", "ClassTwoStudent")</li>
                            <li>@Html.ActionLink("ClassThree", "Index", "ClassThreeStudent")</li>
                            <li>@Html.ActionLink("ClassFour", "Index", "ClassFourStudent")</li>
                            <li>@Html.ActionLink("ClassFive", "Index", "ClassFiveStudent")</li>
                            <li>@Html.ActionLink("ClassSix", "Index", "ClassSixStudent")</li>
                            <li>@Html.ActionLink("ClassSeven", "Index", "ClassSevenStudent")</li>
                            <li>@Html.ActionLink("ClassEight", "Index", "ClassEightStudent")</li>
                            <li>@Html.ActionLink("ClassNine", "Index", "ClassNineStudent")</li>
                            <li>@Html.ActionLink("ClassTen", "Index", "ClassTenStudent")</li>
                        </ul>
                    </li>
                   
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Teachers<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("AllTeacher", "Index", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassOne", "ClassOneTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassTwo", "ClassTwoTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassThree", "ClassThreeTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassFour", "ClassFourTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassFive", "ClassFiveTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassSix", "ClassSixTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassSeven", "ClassSevenTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassEight", "ClassEightTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassNine", "ClassNineTeacher", "AllTheTeacher")</li>
                            <li>@Html.ActionLink("ClassTen", "ClassTenTeacher", "AllTheTeacher")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Courses<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("AllCourse", "Index", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassOne", "ClassOneCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassTwo", "ClassTwoCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassThree", "ClassThreeCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassFour", "ClassFourCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassFive", "ClassFiveCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassSix", "ClassSixCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassSeven", "ClassSevenCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassEight", "ClassEightCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassNine", "ClassNineCourse", "AllTheCourse")</li>
                            <li>@Html.ActionLink("ClassTen", "ClassTenCourse", "AllTheCourse")</li>
                        </ul>
                    </li>
                    
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Notices<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Exams", "Index", "Exam")</li>
                            <li>@Html.ActionLink("Holidays", "Index", "Holiday")</li>
                            <li>@Html.ActionLink("Events", "Index", "Event")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Results<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("ClassOne", "ClassOne", "Home")</li>
                            <li>@Html.ActionLink("ClassTwo", "ClassTwo", "Home")</li>
                            <li>@Html.ActionLink("ClassThree", "ClassThree", "Home")</li>
                            <li>@Html.ActionLink("ClassFour", "ClassFour", "Home")</li>
                            <li>@Html.ActionLink("ClassFive", "ClassFive", "Home")</li>
                            <li>@Html.ActionLink("ClassSix", "ClassSix", "Home")</li>
                            <li>@Html.ActionLink("ClassSeven", "ClassSeven", "Home")</li>
                            <li>@Html.ActionLink("ClassEight", "ClassEight", "Home")</li>
                            <li>@Html.ActionLink("ClassNine", "ClassNine", "Home")</li>
                            <li>@Html.ActionLink("ClassTen", "ClassTen", "Home")</li>
                        </ul>
                    </li>
                    
                </ul>

          
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - School Management System</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
